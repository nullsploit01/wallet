{
  "expo": {
    "name": "Wallet",
    "slug": "wallet",
    "version": "0.4.0",
    "orientation": "portrait",
    "icon": "./src/assets/images/icon.png",
    "scheme": "wallet",
    "userInterfaceStyle": "automatic",
    "splash": {
      "image": "./src/assets/images/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./src/assets/images/adaptive-icon.png",
        "backgroundColor": "#ffffff"
      },
      "package": "in.nullsploit.wallet"
    },
    "web": {
      "bundler": "metro",
      "output": "static",
      "favicon": "./src/assets/images/favicon.png"
    },
    "plugins": [
      [
        "expo-font",
        {
          "fonts": ["./src/assets/fonts/Oswald-Bold.ttf"]
        }
      ],
      [
        "expo-asset",
        {
          "assets": ["./src/assets"]
        }
      ],
      "expo-router",
      "expo-secure-store",
      "expo-local-authentication"
    ],
    "experiments": {
      "typedRoutes": true
    },
    "extra": {
      "router": {
        "origin": false
      },
      "eas": {
        "projectId": "2155a6ce-67cb-475d-9e2d-908b9518b0cd"
      }
    },
    "owner": "nullsploit01"
  }
}
